  "devDependencies": {
    "@eslint/js": "9.18.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.7",
    "eslint": "9.18.0",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
